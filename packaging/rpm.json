{
  "name": "${app_name}",
  "maintainer": "Lateef jackson <lateef.jackson@gmail.com>",
  "license": "LICENSE",
  "url": "https://github.com/lateefj/shylock",
  "files": [
    {
      "from": "build/!arch!/!name!",
      "to": "/usr/bin",
